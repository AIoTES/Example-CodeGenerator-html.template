#set($axioms = $ontology.getAxioms())
#set($props_axioms =$ontology.getAxioms($AxiomType.DATA_PROPERTY_DOMAIN))

#macro(individuals)
    #foreach ( $element in $EntitySearcher.getInstances($class, $ontology) )
        <tr>
            <td>${element.getIRI().getFragment()}</a></td>
            <td>$esc.getText("${element.getIRI().getFragment()}")</td>
        </tr>  
    #end 
#end



#macro(dataprops)

    #foreach ( $item in $ontology.getAxioms($AxiomType.DATA_PROPERTY_RANGE) )
        <tr>
            <td>${item.getIRI().getFragment()}</a></td>
            <td>$esc.getText("${item.getIRI()}")</td>
            <td>$esc.getText("${item.getRange()}")</td>
        </tr>  
    #end     	
#end


<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="UTF-8">
